{
  "name": "dancesuite",
  "version": "1.0.0",
  "description": "Dance Studio Management Software - Built with Bun",
  "scripts": {
    "install:all": "cd backend && bun install && cd ../frontend && bun install",
    "dev:backend": "cd backend && bun run dev",
    "dev:frontend": "cd frontend && bun run dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "prisma:migrate": "cd backend && bunx prisma migrate dev",
    "prisma:generate": "cd backend && bunx prisma generate",
    "prisma:seed": "cd backend && bun run prisma:seed",
    "prisma:studio": "cd backend && bunx prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KertenKerem/DanceSuite.git"
  },
  "keywords": [
    "dance",
    "studio",
    "management",
    "scheduling",
    "billing",
    "bun"
  ],
  "author": "KertenKerem",
  "license": "MIT"
}
